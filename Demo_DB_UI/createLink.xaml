<Window x:Class="Demo_DB_UI.createLink"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo_DB_UI"
        mc:Ignorable="d"
        Title="createLink" Height="216" Width="375">
    <Grid >
        <Label Content="Name" HorizontalAlignment="Left" Margin="98,47,0,0" VerticalAlignment="Top"/>
        <Label Content="LinkUrl" HorizontalAlignment="Left" Margin="98,78,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxLinkName" HorizontalAlignment="Left" Margin="145,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbxLinkUrl" HorizontalAlignment="Left" Margin="145,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="145,157,0,0" VerticalAlignment="Top" Width="42" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="226,157,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
